page{height:100%;background-color:#f5f5f5}.label-container .label-box{margin-bottom:20rpx;background-color:#fff}.label-container .label-box .label-header{display:flex;align-items:center;justify-content:space-between;flex-wrap:nowrap;flex-direction:row;padding:20rpx 30rpx;font-size:28rpx;border-bottom:1px solid #f5f5f5}.label-container .label-box .label-header .label-title{color:#666}.label-container .label-box .label-header .label-edit{color:#30b33a;font-weight:700}.label-container .label-box .label-content{display:flex;align-items:center;justify-content:flex-start;flex-wrap:wrap;flex-direction:row;padding:0 30rpx 30rpx}.label-container .label-box .label-content .label-content-item{position:relative;padding:4rpx 10rpx;margin:24rpx 20rpx 0 0;border:1px solid #666;font-size:28rpx;color:#666}.label-container .label-box .label-content .label-content-item .icon-close{position:absolute;top:-16rpx;right:-16rpx;background-color:#fff;border-radius:50%}.label-container .no-data{width:100%;font-size:28rpx;color:#999;text-align:center;padding:100rpx 0}